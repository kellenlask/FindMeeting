<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="FM.BK" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".people_pane"
            android:label="@string/title_activity_people_pane"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="FM.BK.MainActivity" />
        </activity>
        <activity
            android:name=".SavedPeople"
            android:label="@string/title_activity_saved_people"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="FM.BK.MainActivity" />
        </activity>
        <activity
            android:name=".AvailabilityPage"
            android:label="@string/title_activity_availability_page"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="FM.BK.MainActivity" />
        </activity>
        <activity
            android:name=".NewObAv_Page"
            android:label="@string/title_activity_new_ob_av__page"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="FM.BK.MainActivity" />
        </activity>
    </application>

</manifest>
